{
    "name": "amethyst/data-view-action",
    "type": "library",
    "keywords": ["laravel", "amethyst", "data-view-action"],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "amethyst/core": "0.3.*",
        "amethyst/action": "0.3.*",
        "amethyst/data-view": "0.3.*"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "friendsofphp/php-cs-fixer": "^3.52"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "data-view-action",
            "namespace": "Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\DataViewActionServiceProvider"
            ]
        }
    }
}
